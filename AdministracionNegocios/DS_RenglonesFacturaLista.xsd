        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="RenglonesFacturaListaTableAdapter" GeneratorDataComponentClassName="RenglonesFacturaListaTableAdapter" Name="RenglonesFacturaLista" UserDataComponentName="RenglonesFacturaListaTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="DatosConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Fill" GenerateShortCommands="false" GeneratorSourceName="Fill" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  rfp.idRenglonFacturaProforma
       ,cto.Nombre + ' ' + CASE WHEN ISNULL(pl.Nombre, '') = '' THEN ''
                                ELSE '- Plan ' + pl.Nombre
                                     + CASE WHEN ISNULL(rfp.Titulares, '') = ''
                                            THEN ''
                                            ELSE ' - '
                                                 + CASE WHEN rfp.Titulares = 1
                                                        THEN 'Titulares'
                                                        ELSE 'Adherentes'
                                                   END
                                       END
                           END + ' $'
        + CAST(( ISNULL(rfp.PrecioUnitario, 0)
                 + CASE WHEN ISNULL(rfp.Titulares, '') = '' THEN 0
                        ELSE CASE WHEN rfp.Titulares = 1
                                  THEN ISNULL(pl.Cuota, 0)
                                  ELSE ISNULL(pl.CuotaAdherente, 0)
                             END
                   END ) AS VARCHAR(20)) AS Mostrar
       ,rfp.idPorcentajeIVA
       ,cto.idNegocio
       ,ISNULL(rfp.PrecioUnitario, 0)
        + CASE WHEN ISNULL(rfp.Titulares, '') = '' THEN 0
               ELSE CASE WHEN rfp.Titulares = 1 THEN ISNULL(pl.Cuota, 0)
                         ELSE ISNULL(pl.CuotaAdherente, 0)
                    END
          END AS PrecioUnitario
       ,rfp.idFacturaProforma
FROM    RenglonesFacturaProforma rfp
        INNER JOIN Contratos cto ON rfp.idContrato = cto.idContrato
        LEFT OUTER JOIN Planes pl ON rfp.idPlan = pl.idPlan
WHERE   ( cto.idCliente = @idCliente )</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="idCliente" ColumnName="idCliente" DataSourceName="Datos.dbo.Contratos" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@idCliente" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="idCliente" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
              </DbSource>
