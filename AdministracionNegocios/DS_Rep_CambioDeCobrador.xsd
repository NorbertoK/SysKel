                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  cc.Fecha,
        Cob1.NombApel AS Cobrador1,
        Cob2.NombApel AS Cobrador2,
        cc.Motivo,
        z.Nombre Zona,
        u.NombApel,
        CASE cc.CambiarFuturo
          WHEN 1 THEN 'Sí'
          ELSE 'No'
        END CambiarFuturo
FROM    CambiosDeCobrador cc
        LEFT JOIN dbo.Empleados AS Cob1 ON cc.idCobrador1 = Cob1.idEmpleado
        LEFT JOIN dbo.Empleados AS Cob2 ON cc.idCobrador2 = Cob2.idEmpleado
        LEFT JOIN dbo.Zonas z on cc.idZona = z.idZona
        LEFT JOIN dbo.Usuarios u on cc.idGrabador = u.idUsuario
WHERE   ( cc.idCambioDeCobrador = @idCambioDeCobrador )</CommandText>
