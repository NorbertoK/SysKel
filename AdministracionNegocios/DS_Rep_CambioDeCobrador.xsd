                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     Comprobantes.Prefijo, Comprobantes.NumeroDeComprobante, Comprobantes.NombApel, Comprobantes.Direccion, Comprobantes.Localidad, 
                      Comprobantes.Importe, Comprobantes.Telefono
FROM         CambiosDeCobrador INNER JOIN
