                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  r.Fecha
       ,cob.Iniciales AS Cobrador
       ,r.Caja
       ,rec.Iniciales AS Receptor
       ,u.Iniciales AS Grabador
       ,r.Tipo
       ,r.Detalle
       ,ec.Nombre AS Entidad
FROM    Remesas r
        LEFT OUTER JOIN Empleados  cob ON r.idCobrador = cob.idEmpleado
        LEFT OUTER JOIN Empleados rec  ON r.idReceptor = rec.idEmpleado
        LEFT OUTER JOIN Usuarios u ON r.idGrabador = u.idUsuario
        LEFT OUTER JOIN dbo.EntidadesDeCobranza ec ON r.idEntidadDeCobranza = ec.idEntidadDeCobranza
WHERE   ( r.idRemesa = @idRemesa )</CommandText>
