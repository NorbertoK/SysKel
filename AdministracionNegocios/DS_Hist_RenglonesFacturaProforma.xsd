        --                            AND e.fecha = f.max_fecha
        --       WHERE    e.fecha &lt; @fecha
        --     ) AS g ON a.idRenglonFacturaProforma = g.idRenglonFacturaProforma AND a.Fecha = g.Fecha
        JOIN dbo.RenglonesFacturaProforma rfp ON a.idRenglonFacturaProforma = rfp.idRenglonFacturaProforma
        JOIN dbo.Contratos cto ON rfp.idContrato = cto.idContrato
WHERE   idPlan IS NULL
        AND Estado = 'Activo'
ORDER BY a.Cliente
       ,a.idRenglonFacturaProforma
       ,a.Fecha</CommandText>
