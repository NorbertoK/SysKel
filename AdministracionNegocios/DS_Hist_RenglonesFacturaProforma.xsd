                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  idHist_RenglonesFacturaProforma
       ,a.Cliente
       ,a.idRenglonFacturaProforma
       ,a.[Contrato Antarior]
       ,a.[Contrato Nuevo]
       ,a.[Plan Anterior]
       ,a.[Plan Nuevo]
       ,a.Cantidad_anterior
       ,a.Cantidad_nuevo
       ,a.PrecioUnitario_anterior
       ,a.PrecioUnitario_nuevo
       ,a.RevisarMensualmente_anterior
       ,a.RevisarMensualmente_nuevo
       ,a.Fecha
FROM    View_Hist_RenglonesFacturaProforma a
        JOIN ( SELECT   b.Cliente
                       ,b.Fecha
               FROM     View_Hist_RenglonesFacturaProforma b
                        JOIN ( SELECT   Cliente
                                       ,MAX(Fecha) AS mAX_FECHA
                               FROM     View_Hist_RenglonesFacturaProforma
                               GROUP BY Cliente
                             ) c ON b.Cliente = c.Cliente
                                    AND b.fecha = c.max_fecha
               WHERE    b.fecha &lt; @fecha
             ) AS d ON A.Cliente = d.Cliente
        --JOIN ( SELECT   e.idRenglonFacturaProforma
        --               ,e.Fecha
