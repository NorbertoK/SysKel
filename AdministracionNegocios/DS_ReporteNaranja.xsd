                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  comp.NumeroTarjeta
       ,ISNULL(g.NombreTitularTarjeta, cli.NombreTitularTarjeta) NombreTitularTarjeta
       ,ISNULL(g.NumeroDocumentoTitularTarjeta, cli.NumeroDocumentoTitularTarjeta)NumeroDocumentoTitularTarjeta
       ,dbo.CantidadDeAfiliadosActivosDelGrupo(comp.idGrupo, GETDATE()) AS Afiliados
       ,comp.Saldo
FROM    Comprobantes AS comp
        LEFT JOIN Grupos AS g ON comp.idGrupo = g.idGrupo
        LEFT JOIN clientes cli ON comp.idCliente = cli.idCliente
WHERE   ( comp.idEntidadDeCobranza = 3 )
        AND ( comp.Periodo = @periodo )
        AND ( comp.Saldo &gt; 0 )
        AND ( comp.idTipoDeCobranza = 2 )</CommandText>
